[configuration]

entry_symbol = "GDExtensionInit"
compatibility_minimum = 4.3
${IS_RELOADABLE}

[libraries]

linux.${CONFIG_LOWER}.x86_64 = "lib/${SYSTEM}/lib${PROJECT_NAME}.so"
